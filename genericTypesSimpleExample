class Main {
public static void main(String[] args) {
Integer[] myIntArray = {1,2,3,4};
Character[] myCharArray = {'v','d'};

printMe(myIntArray);
printMe(myCharArray);
}

public static <T> void printMe(T[] x) {

for(T b : x)
System.out.printf("'%s'", b);
System.out.println();
//System.out.printf('%s ', b); //BREAKS ADDING THIS LINE
System.out.println();

}

}
